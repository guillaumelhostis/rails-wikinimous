

<div class="title">
<h1>New Chuck Norris quotes</h1>
</div>
<div class="crud">
<div class="btn btn-primary"><%= link_to "New article", new_article_path %></div>
</div>
<div class="articles">
<ul>
  <% @articles.each do |article| %>
  <li>
    <div class="link">
    <li> <div class="link"><strong><%= link_to article.title, article_path(article) %></strong></div></li>

    <li><%=article.content  %></li>
  </li>
<% end %>
</ul>
</div>
